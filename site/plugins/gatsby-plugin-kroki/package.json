{
  "name": "gatsby-plugin-kroki",
  "version": "1.0.0",
  "description": "Gatsby plugin to process file nodes into kroki nodes and download the rendered diagrams",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-transformer-plugin",
    "kroki",
    "diagram"
  ],
  "author": "Paulo Menin <paulomenin@gmail.com>",
  "packageManager": "yarn@3.2.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "peerDependencies": {
    "gatsby": "^4.0.0"
  },
  "dependencies": {
    "gatsby-source-filesystem": "^4.18.0",
    "gray-matter": "^4.0.3",
    "pako": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "babel-preset-gatsby-package": "^2.18.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.0",
    "prettier": "2.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./*.js"
    ],
    "coverageThreshold": {
